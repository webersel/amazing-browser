{
  "version": 2,
  "builds": [
    {
      "src": "*.php",
      "use": "@vercel/php"
    },
    {
      "src": "ws-proxy.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/ws-proxy/(.*)",
      "dest": "/ws-proxy.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.php"
    }
  ]
}